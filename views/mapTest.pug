<!DOCTYPE html>
<html>
<head>
script.
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCKUZ24d2NciHv2zeN9S1FbXmO9or-hpko&callback=initMap">
style.
    #map {
        height: 400px;
        width: 100%;
    }

</head>
<body>
<h3>My Google Maps Demo</h3>
<div id="map"></div>
script.
    function initMap() {
        var uluru = {lat: -25.363, lng: 131.044};
        var map = new google.maps.Map(document.getElementById('map'), {
            zoom: 4,
    center: uluru
    });
    var marker = new google.maps.Marker({
     position: uluru,
     map: map
    });
    }

</body>
</html>